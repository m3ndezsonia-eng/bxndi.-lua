-- Mobile FPS Boost Script-- Seguro para Roblox (LocalScript)local Lighting = game:GetService("Lighting")local UserSettings = UserSettings():GetService("UserGameSettings")local Terrain = workspace:FindFirstChildOfClass("Terrain")-- Forzar gr치ficos bajosUserSettings.SavedQualityLevel = Enum.SavedQualitySetting.QualityLevel1-- Iluminaci칩n ultra b치sicaLighting.GlobalShadows = falseLighting.FogEnd = 1e10Lighting.Brightness = 1Lighting.EnvironmentDiffuseScale = 0Lighting.EnvironmentSpecularScale = 0-- Quitar efectos visualesfor _, effect in ipairs(Lighting:GetChildren()) do	if effect:IsA("PostEffect") then		effect.Enabled = false	endend-- Optimizar agua (muy pesado en m칩vil)if Terrain then	Terrain.WaterWaveSize = 0	Terrain.WaterWaveSpeed = 0	Terrain.WaterReflectance = 0	Terrain.WaterTransparency = 1end-- Simplificar partesfor _, v in ipairs(workspace:GetDescendants()) do	if v:IsA("BasePart") then		v.Material = Enum.Material.Plastic		v.Reflectance = 0	endendprint("游님 FPS Boost m칩vil activado")
